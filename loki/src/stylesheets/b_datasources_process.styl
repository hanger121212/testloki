@import "./config.styl"
#page_datasources_process
    padding-top: 30px
    .icon
        fill currentColor
        width: 20px
        height: 20px
        display: inline-block
        color: #fff
        margin 0
    .stepbox
        padding-bottom: 20px;
    .stepbox.disabled
        .el-collapse-item__header
            cursor: unset
    .el-collapse-item__arrow
        display: none
    .el-collapse-item__header
        height: 40px;
        background-color: #e6e6e6
        .iconbox
            padding 10px;
            background-color: #b3b3b3
            display: inline-block
            line-height: 0;
            border-top-left-radius: 2px
            border-bottom-left-radius: 2px
            vertical-align: top
            .edit
                display: none;
            .noedit
                display: block
                span
                    line-height: 20px
                    width: 20px
                    color: #fff
                    margin 0
                    font-size: 24px
                    position: relative;
                    left: -2px;
        span
            display: inline-block
            margin-left: 20px
            font-size: 17px
            line-height: 40px
            vertical-align: top
            color: #666
    .is-active
        .el-collapse-item__header
            background-color: #dbdbdb
            .iconbox
                display: inline-block
                background-color: #007bc8
                border-bottom-left-radius: 0
                .edit
                    display: inline-block;
                .noedit
                    display: none
            span
                color #333
    .stepbox.ok
        .el-collapse-item:not(.is-active)
            .el-collapse-item__header
                .iconbox
                    background-color: #1f992c
    .el-upload-list
        display: none;
    .el-collapse-item__content
        padding: 30px 40px;
    .nofile .uploadbtn .text
        color #ccc
    .uploadbtn
        width: 400px;
        padding: 0;
        height: 32px
        border-color: #b3b3b3
        position: relative
        background-color: transparent
        .text
            position: relative;
            display: inline-block
            float left
            line-height: 32px
            font-size: 14px
            color #666
            padding-left: 20px
        .iconbox
            display: inline-block
            width: 34px
            line-height: 30px
            float right
            background-color: #b3b3b3
            .icon
                vertical-align: middle
    .progressbar
        width: 398px;
        position: absolute;
        border 1px solid #b3b3b3
        left: 0
        top: 0
        height: 30px
        pointer-events: none
        border-radius: 2px
        background-color: #fff
        .percent
            width: 0px;
            background-color: #b3e2ff
            height: 26px
            border 2px solid #fff
            transition: all 0.2s
            text-align: left
            span 
                color: #666
                margin-left: 20px
                position: absolute;
                left: 0;
                top: 3px;
    .uploadbtn:focus
        color: inherit
        border-color: #b3b3b3
        background-color: #fff;
    .upload__tip
        padding-bottom: 30px
    .nextstep
        background-color: #ff9000
        border: 0
        padding: 0
        width: 100px
        text-align: center
        span
            font-size: 14px
            line-height: 30px
            color #fff
    .nextstep:hover
        background-color: #ff7700
    .test
        background-color: #018ee6
        border: 0
        padding: 0
        width: 100px
        text-align: center
        span
            font-size: 14px
            line-height: 30px
            color #fff
    .test:hover
        background-color: #017bc8
    .desctext
        font-size: 12px
        font-weight: bold
        line-height: 12px
        color: #333
        padding-bottom: 10px
        .runsql
            margin-left: 10px
    .el-input__inner
        height: 32px
        width: 400px
        border-color: #b3b3b3
    .el-textarea__inner
        width: 400px
        border-color: #b3b3b3
    .sheetselect
        margin-bottom: 20px
    .sqlarea
        textarea
            height: 100px
            width: 100%
    .el-tag--small
        margin-bottom: 1px
        margin-top: 1px
        line-height: 20px
        height: 20px
    .el-select__tags
        input
            height: 20px
            vertical-align: top
    .sheetselect.short
        .el-input__inner
            width: 160px
    .el-input.is-focus
        .el-input__inner
            border-color: #80ceff
    .el-table
        margin-bottom 20px
        font-size: 12px
        color #666
        .el-select,.el-input__inner
            width: 100%
        input
            font-size: 12px
            height: 30px
        .small
            padding-bottom: 2px
            input
                vertical-align: top
            span
                line-height: 28px
                vertical-align: top
                display: block
        .el-input.fix
            margin-top: 5px;
        .el-input.error
            input
                border-color: #f56c6c
    
    .preview.el-table td, .preview.el-table th
        padding: 0 20px
        height: 34px
    .preview.el-table .cell
        line-height: 40px
        white-space: nowrap;
    .rowsetting tr td:nth-child(1)
        padding 0 17px
    .el-table th .cell
        line-height: 30px
    .el-table td, .el-table th
        padding: 10px 20px
        height: 34px
    .el-table th
        color #333333
        font-weight: bold
        background-color: #d9d9d9
    .el-table--striped .el-table__body tr.el-table__row--striped td
        background-color: #f2f2f2
    .fixinput.el-input .el-input__inner
        width: 100%